package ict.22.ClassNames;

public class SumClass {

    // static instance
    public static SumClass instance = new SumClass();

    public void calculateSum() {
        double sum = 0.0;
        double i = 1.0;

        do {
            sum = sum + i;
            i = i - 0.1;
        } while (i >= 0.1);

        System.out.println("Sum = " + sum);
    }
}
